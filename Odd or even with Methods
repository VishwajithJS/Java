import java.util.*;
class Main {
    static void eoo(int a){
        if(a%2==0){
            System.out.println("Even");
        }
        else{
            System.out.println("ODD");
        }
    }
    public static void main(String[] args) {
        System.out.println("Enter the number:");
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        eoo(a);
    }
}
