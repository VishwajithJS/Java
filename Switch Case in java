import java.util.Scanner;
class Main{
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter a number:");
        int a = sc.nextInt();
        switch(a){
            case 1:
                System.out.print("One");
                break;
            case 2:
                System.out.print("Two");
                break;
            default:
                System.out.print(">2");
        }
    }
}
