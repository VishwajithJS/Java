class Main {
    public static void main(String[] args) {
        int[]arr = {45,23,2,25};
        for(int i=0;i<arr.length;i++){
            for(int j=i;j<arr.length;j++){
                if(arr[i]>arr[j]){
                    int temp = arr[i];
                    arr[i]=arr[j];
                    arr[j]=temp;
                }
            }
        }
        for(int a:arr) System.out.print(a+" ");
    }
}
