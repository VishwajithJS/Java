class Main {
    public static void main(String[] args) {
        int[]arr = {10,20,30,40,50};
        int small = arr[0];
        for(int i=0;i<4;i++){
            if(arr[i+1]<small){
                small = arr[i+1];
            }
        }
        System.out.println("The smallest element of the array is"+" "+small);
    }
}
