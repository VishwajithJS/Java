import java.util.*;
class Main{
    static void ask(){
        System.out.println("Enter a number =");
    }
    static void print(int n){
        System.out.println(n);
    }
    static void add(int a,int b){
        print(a+b);
    }
    public static void main(String[] args){
        Scanner sr = new Scanner(System.in);
        ask();
        int num1 =sr.nextInt();
        ask();
        int num2=sr.nextInt();
        add(num1,num2);
    }
}
