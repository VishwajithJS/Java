class Main{
    public static void main(String[] args){
    secret ritvik = new secret();    
    ritvik.view();
    ritvik.update(696969);
    ritvik.view();
    }
}
class secret{
    private int PIN= 1234;
    void view(){
        System.out.println(this.PIN);
    }
    void update(int num){
        this.PIN = num;
        System.out.println("PIN updated Successfully");
    }
}
